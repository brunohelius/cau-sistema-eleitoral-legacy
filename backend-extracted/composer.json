{
	"name" : "cau/carteira-api",
	"description" : "CAU/BR",
	"keywords" : [
		"framework",
		"laravel",
		"lumen"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php": ">=7.1.3",
		"ext-SimpleXML": "*",
		"ext-ctype": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-json": "*",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-xml": "*",
		"ext-xmlreader": "*",
		"ext-xmlwriter": "*",
		"ext-zip": "*",
		"ext-zlib": "*",
		"barryvdh/laravel-dompdf": "^0.8.6",
		"barryvdh/laravel-snappy": "^0.4.7",
		"darkaonline/swagger-lume": "5.6.*",
		"doctrine/annotations": "v1.7.0",
		"doctrine/collections": "v1.6.4",
		"doctrine/common": "v2.11.0",
		"doctrine/dbal": "v2.9.2",
		"doctrine/lexer": "1.1.0",
		"doctrine/orm": "v2.6.3",
		"doctrine/persistence": "v1.3",
		"doctrine/reflection": "v1.0.0",
		"dompdf/dompdf": "^0.8.6",
		"endroid/qr-code": "^3.9",
		"firebase/php-jwt": "^5.0",
		"guzzlehttp/guzzle": "^6.5",
		"h4cc/wkhtmltopdf-amd64": "^0.12.4",
		"illuminate/mail": "5.8.*",
		"illuminate/redis": "5.8.*",
		"intervention/image": "^2.5",
		"jms/serializer-bundle": "^3.4",
		"laravel-doctrine/orm": "1.4.10",
		"laravel/lumen-framework": "5.8.*",
		"markbaker/complex": "^1.4",
		"markbaker/matrix": "^1.2",
		"marvinlabs/laravel-discord-logger": "^1.1",
		"mpdf/mpdf": "^7.0",
		"nesbot/carbon": "^2.35",
		"phpoffice/phpspreadsheet": "1.9.0",
		"phpoffice/phpword": "^0.17.0",
		"predis/predis": "^1.1",
		"psr/simple-cache": "^1.0",
		"swiftmailer/swiftmailer": "6.2.*",
		"twig/twig": "v1.34.4",
		"tymon/jwt-auth": "^1.0"
	},
	"require-dev" : {
		"flipbox/lumen-generator": "^6.0",
		"fzaninotto/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^7.0"
	},
	"autoload" : {
		"classmap" : [
			"database/seeds",
			"database/factories"
		],
		"psr-4" : {
			"App\\" : "app/",
			"PhpOffice\\PhpSpreadsheet\\": "src/PhpSpreadsheet"
		}
	},
	"autoload-dev" : {
		"classmap" : [
			"tests/"
		],
		"psr-4": {
			"PhpOffice\\PhpSpreadsheetTests\\": "tests/PhpSpreadsheetTests"
		}
	},
	"scripts" : {
		"post-root-package-install" : "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
	},
	"config" : {
		"preferred-install" : "dist",
		"sort-packages" : true,
		"optimize-autoloader" : true
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"repositories": {
		"packagist.org": {
			"type": "composer",
			"url": "https://packagist.org"
		}
	}
}
